  "dependencies": {
    "axios": "^0.20.0",
    "dedent": "^0.7.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.6.0",
    "fastify-helmet": "^5.0.3",
    "fastify-raw-body": "^3.0.0",
    "fastify-sensible": "^3.0.1",
